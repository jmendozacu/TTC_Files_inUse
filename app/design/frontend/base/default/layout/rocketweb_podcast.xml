<?xml version="1.0"?>
<layout version="0.1.0">
    <default>
    </default>
    <podcasts_index_index>
        <reference name="head">
            <action method="addItem"><type>skin_css</type><name>rocketweb_podcast/css/style.css</name></action>
            <action method="addItem"><type>skin_js</type><name>rocketweb_podcast/js/audiojs/audio.min.js</name></action>
        </reference>
        <reference name="content">
            <block type="podcast/list" name="podcast_list" template="rocketweb_podcast/podcast_list.phtml" />
        </reference>
    </podcasts_index_index>
    <podcasts_podcast_view>
        <reference name="head">
            <action method="addItem"><type>skin_css</type><name>rocketweb_podcast/css/style.css</name></action>
            <action method="addItem"><type>skin_js</type><name>rocketweb_podcast/js/audiojs/audio.min.js</name></action>
        </reference>
        <reference name="content">
            <block type="podcast/podcast" name="podcast" template="rocketweb_podcast/podcast.phtml" />
        </reference>
    </podcasts_podcast_view>
</layout> 